{
    "name": "klipper/rope",
    "type": "composer-plugin",
    "description": "Composer plugin for Klipper",
    "keywords" : ["klipper", "rope"],
    "homepage" : "https://github.com/klipperdev/rope",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@klipper.dev"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "composer-plugin-api": "^1.1",
        "symfony/flex": "^1.2.0"
    },
    "require-dev": {
        "composer/composer": "^1.6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Klipper\\Rope\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "class": "Klipper\\Rope\\Rope"
    }
}
